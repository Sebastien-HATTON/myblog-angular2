{
  "name": "myblog-angular2",
  "version": "0.1.0",
  "description": "Angular2 and Drupal",
  "scripts": {
    "clean-install": "rm -rf node_modules && rm -rf typings && npm install",
    "watch": "./node_modules/.bin/webpack --watch",
    "prebuild": "./node_modules/.bin/rimraf dist",
    "build": "./node_modules/.bin/webpack",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.js",
    "prestart": "npm run build",
    "start": "./node_modules/.bin/nodemon dist/server/index.js",
    "predebug": "npm run build",
    "debug:start": "npm run build && npm run debug:server",
    "debug:server": "node-nightly --inspect --debug-brk dist/server/index.js"
  },
  "repository": {
    "type": "git",
